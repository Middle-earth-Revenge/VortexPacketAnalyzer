{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "overrides": [
    {
      "files": [
        "**/scripts/*"
      ],
      "rules": {
        "import/no-extraneous-dependencies": [
          "error",
          {
            "devDependencies": true
          }
        ]
      }
    }
  ],
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off"
  }
}